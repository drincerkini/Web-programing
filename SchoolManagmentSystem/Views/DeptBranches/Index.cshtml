@model IEnumerable<SchoolManagmentSystem.Models.DeptBranch>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Department)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Branch)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Department.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Branch.BranchID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DeptBranchID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DeptBranchID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DeptBranchID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
