@{
    Layout = "_AdminLayout";
    ViewData["title"] = "Index";
}

<br />
<h1 class="d-flex justify-content-center">Roles</h1>
<hr />
<br />

<div class="text-center">

    <table class="table border-1">
        <tbody>

            @foreach (var role in ViewBag.roles)
            {
                <tr>
                    <td>@role</td>
                </tr>
            }
        </tbody>
    </table>
</div>
